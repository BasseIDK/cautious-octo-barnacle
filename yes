-- Scripts

local function HDYTX_fake_script() -- EatSimGui.MainScript 
	local script = Instance.new('LocalScript', EatSimGui)

	local UserInputService = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	local CoreGui = game:GetService("CoreGui")
	
	local MainFrame = script.Parent.Frame
	local DragFrame = MainFrame.DragFrame
	
	local CanDrag = true
	
	local Dragging
	local DragInput
	local DragStart
	local StartPos
	
	local Activated = {
		AutoFarm = false,
		AutoFarmCover = false,
		EatSounds = false,
		Rolling = false
	}
	
	local RollingAnim = Instance.new("Animation")
	RollingAnim.AnimationId = "rbxassetid://14531654819"
	
	local playAnim
	
	local function update(input)
		local delta = input.Position - DragStart
		local dragTime = 0.04
		local SmoothDrag = {}
		if CanDrag then
			SmoothDrag.Position = UDim2.new(StartPos.X.Scale, StartPos.X.Offset + delta.X, StartPos.Y.Scale, StartPos.Y.Offset + delta.Y)
			local dragSmoothFunction = TweenService:Create(MainFrame, TweenInfo.new(dragTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), SmoothDrag)
			dragSmoothFunction:Play()
		end
	end
	
	DragFrame.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			Dragging = true
			DragStart = Input.Position
			StartPos = MainFrame.Position
			Input.Changed:Connect(function()
				if Input.UserInputState == Enum.UserInputState.End then
					Dragging = false
				end
			end)
		end
	end)
	DragFrame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and Dragging and MainFrame.Size then
			update(input)
		end
	end)
	
	for i,v in pairs(MainFrame.ScrollingFrame:GetChildren()) do
		if v:IsA("Frame") then
			v.TextButton.MouseButton1Click:Connect(function()
				if v.Name == "AutoFarm" then
					if not Activated.AutoFarm then
						Activated.AutoFarm = true
						v.TextButton.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
						v.TextButton.Text = "On"
						game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
					else
						Activated.AutoFarm = false
						v.TextButton.BackgroundColor3 = Color3.fromRGB(150, 150, 150)
						v.TextButton.Text = "Off"
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(167.510544, 346713.75, -292.525909, 0.0420858115, 7.91287369e-10, -0.999113977, -4.23205748e-10, 1, 7.74162345e-10, 0.999113977, 3.90249555e-10, 0.0420858115)
						game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
					end
				elseif v.Name == "AutoFarmCover" then
					if not Activated.AutoFarmCover then
						Activated.AutoFarmCover = true
						v.TextButton.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
						v.TextButton.Text = "On"
					else
						Activated.AutoFarmCover = false
						v.TextButton.BackgroundColor3 = Color3.fromRGB(150, 150, 150)
						v.TextButton.Text = "Off"
					end
				elseif v.Name == "EatSounds" then
					if not Activated.EatSounds then
						Activated.EatSounds = true
						v.TextButton.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
						v.TextButton.Text = "On"
					else
						Activated.EatSounds = false
						v.TextButton.BackgroundColor3 = Color3.fromRGB(150, 150, 150)
						v.TextButton.Text = "Off"
					end
				elseif v.Name == "Rolling" then
					if not Activated.Rolling then
						Activated.Rolling = true
						v.TextButton.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
						v.TextButton.Text = "On"
						playAnim = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(RollingAnim)
						playAnim:Play()
					else
						Activated.Rolling = false
						v.TextButton.BackgroundColor3 = Color3.fromRGB(150, 150, 150)
						v.TextButton.Text = "Off"
						playAnim:Stop()
					end
				end
			end)
		end
	end
	
	task.spawn(function()
		while task.wait() do
			if Activated.AutoFarm then
				if workspace:FindFirstChild("GameObjects") then
					local FoodsFolder = workspace.GameObjects:FindFirstChild("Foods")
					if FoodsFolder then
						local Foods = workspace.GameObjects.Foods:GetChildren()
						local RandomFood = Foods[math.random(1, #Foods)]
						if RandomFood:FindFirstChildWhichIsA("MeshPart") then
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = RandomFood:FindFirstChildWhichIsA("MeshPart").CFrame
						end
					end
				end
				if Activated.AutoFarmCover then
					script.Parent.AutoFarmCover.Visible = true
				end
				if Activated.EatSounds == true then
					for i,v in pairs(game.Players.LocalPlayer.Character.HumanoidRootPart:GetChildren()) do
						if v.Name == "Eat" then
							v:Destroy()
						end
					end
				end
			end
			if not Activated.AutoFarmCover then
				script.Parent.AutoFarmCover.Visible = false
			end
		end
	end)
	
	game.Players.LocalPlayer.CharacterAdded:Connect(function()
		if Activated.Rolling == true then
			game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
			wait(0.1)
			playAnim = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(RollingAnim)
			playAnim:Play()
		end
	end)
	
	script.Parent.AutoFarmCover.TextButton.MouseButton1Click:Connect(function()
		Activated.AutoFarmCover = false
		MainFrame.ScrollingFrame.AutoFarmCover.TextButton.BackgroundColor3 = Color3.fromRGB(150, 150, 150)
		MainFrame.ScrollingFrame.AutoFarmCover.TextButton.Text = "Off"
	end)
end
coroutine.wrap(HDYTX_fake_script)()
